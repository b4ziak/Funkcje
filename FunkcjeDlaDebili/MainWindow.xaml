        <Label x:Name="lblItteration" Content="Itteracje" HorizontalAlignment="Left" Margin="490,64,0,0" VerticalAlignment="Top" Width="81"/>
        <Label x:Name="lblError" Content="&#xD;&#xA;" HorizontalAlignment="Left" Margin="136,162,0,0" VerticalAlignment="Top" Height="55" Width="232" Background="Transparent" Foreground="Red"/>
        <TextBox x:Name="txtRound" HorizontalAlignment="Center" Margin="0,82,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="120" Height="34"/>
        <Label Content="Dokładność" HorizontalAlignment="Center" Margin="0,56,0,0" VerticalAlignment="Top" Width="82"/>

    </Grid>
</Window>
